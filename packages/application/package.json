{
    "name": "mercury-application",
    "version": "3.0.0-rc.1",
    "description": "Mercury Jupyter application.",
    "private": true,
    "keywords": [
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "homepage": "https://github.com/mljar/mercury-extension",
    "bugs": {
        "url": "https://github.com/mljar/mercury-extension/issues"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "MLJAR Sp. z o.o.",
        "email": "contact@mljar.com"
    },
    "files": [
        "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
        "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "style": "style/index.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/mljar/mercury-extension.git"
    },
    "scripts": {
        "build": "yarn run build:prod",
        "build:prod": "tsc -b",
        "build:test": "tsc --build tsconfig.test.json",
        "clean": "rimraf lib tsconfig.tsbuildinfo",
        "watch": "tsc -b --watch --preserveWatchOutput"
    },
    "dependencies": {
        "@jupyterlab/application": "^4.2.0",
        "@jupyterlab/apputils": "^4.3.0",
        "@jupyterlab/cells": "^4.2.0",
        "@jupyterlab/codeeditor": "^4.2.0",
        "@jupyterlab/coreutils": "^6.2.0",
        "@jupyterlab/docmanager": "^4.2.0",
        "@jupyterlab/docregistry": "^4.2.0",
        "@jupyterlab/notebook": "^4.2.0",
        "@jupyterlab/rendermime-interfaces": "^3.8.3",
        "@jupyterlab/translation": "^4.2.0",
        "@lumino/coreutils": "^2.0.0",
        "@lumino/messaging": "^2.0.0",
        "@lumino/signaling": "^2.0.0",
        "@lumino/widgets": "^2.0.1",
        "@mljar/mercury-extension": "workspace:*",
        "@mljar/mercury-tokens": "workspace:*"
    },
    "devDependencies": {
        "@jupyterlab/builder": "^4.0.0",
        "@mljar/mercury-extension": "workspace:*",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "mkdirp": "^1.0.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.0.0",
        "rimraf": "^5.0.0",
        "typescript": "~5.0.4"
    },
    "sideEffects": [
        "style/*.css",
        "style/index.js"
    ],
    "styleModule": "style/index.js",
    "publishConfig": {
        "access": "public"
    },
    "jupyterlab": {
        "coreDependency": true,
        "sharedPackages": {
            "@jupyter-widgets/base": {
                "bundled": false,
                "singleton": true
            },
            "@jupyter-widgets/jupyterlab-manager": {
                "bundled": false,
                "singleton": true
            },
            "@jupyter/react-components": {
                "bundled": false,
                "singleton": true
            },
            "@jupyter/web-components": {
                "bundled": false,
                "singleton": true
            },
            "@jupyter/ydoc": {
                "bundled": false,
                "singleton": true
            },
            "@microsoft/fast-colors": {
                "bundled": false,
                "singleton": true
            },
            "@microsoft/fast-element": {
                "bundled": false,
                "singleton": true
            },
            "@microsoft/fast-foundation": {
                "bundled": false,
                "singleton": true
            },
            "@microsoft/fast-web-utilities": {
                "bundled": false,
                "singleton": true
            },
            "yjs": {
                "bundled": false,
                "singleton": true
            }
        }
    }
}
